    "preview": "yarn build-preview && vite preview",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@fedimint/fedimint-client-wasm": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "typescript": "^5.2.2",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm-pack": "^0.1.12"
  }
}
